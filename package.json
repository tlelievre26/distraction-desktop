{
  "name": "distraction-desktop",
  "version": "1.0.0",
  "description": "Desktop component for the DistrAction app usage tracking tool",
  "main": "src/main.js",
  "scripts": {
    "test:integration": "playwright test",
    "test:unit": "jest",
    "electron": "electron .",
    "start": "webpack serve --config webpack.config.js --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "lint": "eslint .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "npm-run-all --parallel start electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tlelievre26/distraction-desktop.git"
  },
  "author": "Taylor Le Lievre, Wilson Wong, Stephanie Zhan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tlelievre26/distraction-desktop/issues"
  },
  "homepage": "https://github.com/tlelievre26/distraction-desktop#readme",
  "build": {
    "appId": "com.distrAction.app",
    "productName": "DistrAction",
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "nsis",
      "icon": "public/clock-icon.png"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@eslint/js": "^9.10.0",
    "@playwright/test": "^1.46.1",
    "@types/node": "^22.5.2",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "electron": "^32.0.2",
    "electron-builder": "^25.0.5",
    "eslint": "^9.9.1",
    "eslint-config-eslint": "^11.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^8.1.1",
    "sass": "^1.78.0",
    "sass-loader": "^16.0.1",
    "style-loader": "^4.0.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "electron-log": "^5.2.0",
    "jquery": "^3.7.1",
    "module-alias": "^2.2.3",
    "popper.js": "^1.16.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "@influxdata/influxdb-client": "^1.35.0"
  },
  "_moduleAliases": {
    "@": "src"
  }
  
}
